{
	"config": [{
			"role": [{
				"role-name": "kerndaten-reader",
				"description": "Read access to kerndaten"
			}]
		},
		{
			"role": [{
				"role-name": "kerndaten-rest-reader",
				"description": "Read access to kerndaten through ReST",
				"role": ["kerndaten-reader", "rest-reader"]
			}]
		},
		{
			"role": [{
				"role-name": "kerndaten-writer",
				"description": "Write access to kerndaten",
				"permission": [{
					"role-name": "kerndaten-reader",
					"capability": "read"
				}]
			}]
		},
		{
			"op": "update",
			"role": [{
				"role-name": "kerndaten-writer",
				"description": "Write access to kerndaten",
				"permission": [{
						"role-name": "kerndaten-writer",
						"capability": "update"
					},
					{
						"role-name": "kerndaten-reader",
						"capability": "read"
					}
				]
			}]
		},
		{
			"role": [{
				"role-name": "kerndaten-rest-writer",
				"description": "Write access to kerndaten through ReST",
				"role": ["kerndaten-writer", "rest-writer"]
			}]
		},
		{
			"role": [{
				"role-name": "kerndaten-crawler",
				"description": "Role for the kerndaten crawler process",
				"role": ["kerndaten-rest-reader", "kerndaten-rest-writer"]
			}]
		},
		{
			"privilege": [{
					"privilege-name": "kerndaten-uri",
					"action": "/kerndaten/",
					"kind": "uri",
					"role": ["kerndaten-writer"]
				},
				{
					"privilege-name": "crawler-uri",
					"action": "/crawler/",
					"kind": "uri",
					"role": ["kerndaten-crawler"]
				}
			]
		},
		{
			"user": [{
				"user-name": "kerndaten-crawler",
				"description": "Kerndaten Crawler",
				"password": "BesserWisser",
				"role": [
					"kerndaten-crawler"
				]
			}]
		},
		{
			"forest": [{
					"forest-name": "kerndaten-1",
					"enabled": true,
					"data-directory": "",
					"large-data-directory": "",
					"fast-data-directory": ""
				}, {
					"forest-name": "kerndaten-2",
					"enabled": true,
					"data-directory": "",
					"large-data-directory": "",
					"fast-data-directory": ""
				}, {
					"forest-name": "kerndaten-3",
					"enabled": true,
					"data-directory": "",
					"large-data-directory": "",
					"fast-data-directory": ""
				},
				{
					"forest-name": "kerndaten-schemas-1",
					"enabled": true,
					"data-directory": "",
					"large-data-directory": "",
					"fast-data-directory": ""
				},
				{
					"forest-name": "kerndaten-triggers-1",
					"enabled": true,
					"data-directory": "",
					"large-data-directory": "",
					"fast-data-directory": ""
				},
				{
					"forest-name": "kerndaten-crawler-modules-1",
					"enabled": true,
					"data-directory": "",
					"large-data-directory": "",
					"fast-data-directory": ""
				}
			]
		},
		{
			"database": [{
					"database-name": "kerndaten-crawler-modules",
					"security-database": "Security",
					"forest": ["kerndaten-crawler-modules-1"]
				},
				{
					"database-name": "kerndaten-schemas",
					"security-database": "Security",
					"forest": ["kerndaten-schemas-1"]
				},
				{
					"database-name": "kerndaten-triggers",
					"security-database": "Security",
					"forest": ["kerndaten-triggers-1"]
				},
				{
					"database-name": "kerndaten",
					"security-database": "Security",
					"schema-database": "kerndaten-schemas",
					"triggers-database": "kerndaten-triggers",
					"triple-index": true,
					"default-ruleset": [{
						"location": "/rules/kerndaten.rules"
					}],
					"forest": ["kerndaten-1", "kerndaten-2", "kerndaten-3"]
				}
			]
		},
		{
			"server": [{
					"server-name": "deepick-kerndaten",
					"group-name": "Default",
					"server-type": "http",
					"enabled": true,
					"root": "/home/hans/kerndaten-crawler/modules",
					"port": 9849,
					"content-database": "kerndaten",
					"default-user": "nobody"
				},
				{
					"server-name": "deepick-kerndaten.odbc",
					"group-name": "Default",
					"server-type": "odbc",
					"enabled": true,
					"root": "/",
					"port": 9850,
					"content-database": "kerndaten",
					"modules-database": "kerndaten-crawler-modules"
				},
				{
					"server-name": "deepick-kerndaten.rest",
					"group-name": "Default",
					"server-type": "http",
					"enabled": true,
					"root": "/",
					"port": 9848,
					"public-port": false,
					"webDAV": false,
					"execute": true,
					"display-last-login": false,
					"address": "0.0.0.0",
					"backlog": 512,
					"threads": 32,
					"request-timeout": 30,
					"keep-alive-timeout": 5,
					"session-timeout": 3600,
					"max-time-limit": 3600,
					"default-time-limit": 600,
					"max-inference-size": 500,
					"default-inference-size": 100,
					"static-expires": 3600,
					"pre-commit-trigger-depth": 1000,
					"pre-commit-trigger-limit": 10000,
					"collation": "http://marklogic.com/collation/",
					"coordinate-system": "wgs84",
					"authentication": "digest",
					"internal-security": true,
					"concurrent-request-limit": 0,
					"compute-content-length": true,
					"file-log-level": "info",
					"opsdirector-applog-level": "disabled",
					"log-errors": true,
					"debug-allow": true,
					"profile-allow": true,
					"default-xquery-version": "1.0-ml",
					"multi-version-concurrency-control": "contemporaneous",
					"distribute-timestamps": "fast",
					"output-sgml-character-entities": "none",
					"output-encoding": "UTF-8",
					"output-method": "default",
					"output-byte-order-mark": "default",
					"output-cdata-section-namespace-uri": "",
					"output-cdata-section-localname": null,
					"output-doctype-public": "",
					"output-doctype-system": "",
					"output-escape-uri-attributes": "default",
					"output-include-content-type": "default",
					"output-indent": "default",
					"output-indent-untyped": "default",
					"output-indent-tabs": "default",
					"output-media-type": "",
					"output-normalization-form": "none",
					"output-omit-xml-declaration": "default",
					"output-standalone": "omit",
					"output-undeclare-prefixes": "default",
					"output-version": "",
					"output-include-default-attributes": "default",
					"default-error-format": "json",
					"error-handler": "/MarkLogic/rest-api/error-handler.xqy",
					"url-rewriter": "/MarkLogic/rest-api/rewriter.xml",
					"rewrite-resolves-globally": true,
					"ssl-allow-sslv3": true,
					"ssl-allow-tls": true,
					"ssl-disable-sslv3": false,
					"ssl-disable-tlsv1": false,
					"ssl-disable-tlsv1-1": false,
					"ssl-disable-tlsv1-2": false,
					"ssl-hostname": "",
					"ssl-ciphers": "ALL:!LOW:@STRENGTH",
					"ssl-require-client-certificate": true,
					"ssl-client-issuer-authority-verification": false,
					"content-database": "kerndaten",
					"modules-database": "kerndaten-crawler-modules",
					"default-user": "nobody"
				}
			]
		}
	]
}