{
  "modules": {
    ".*": {
      "permissions": [
        { "role-name": "kerndaten-writer", "capabilities": ["execute"] }
      ]
    },
    "\\.sparql$": {
      "format": "text",
      "contentType": "text/plain"
    }
  },
  "schemas": {
    ".*": {
      "permissions": [
        { "role-name": "kerndaten-reader", "capabilities": ["read"] }
      ],
      "format": "text",
      "contentType": "text/plain"
    }
  }
}
